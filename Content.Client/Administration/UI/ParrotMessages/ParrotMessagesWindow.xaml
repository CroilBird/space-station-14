<ui:FancyWindow xmlns="https://spacestation14.io"
                xmlns:parrotmessages="clr-namespace:Content.Client.Administration.UI.ParrotMessages"
                xmlns:ui="clr-namespace:Content.Client.UserInterface.Controls"
                VerticalExpand="True" HorizontalExpand="True"
                Title="{Loc parrot-messages-title}"
                SetSize="800 400">
    <PanelContainer StyleClasses="BackgroundDark">
        <TabContainer Name="MessageTabContainer">
            <parrotmessages:ParrotMessageList
                Name="ActiveList"
                Access="Protected">
            </parrotmessages:ParrotMessageList>
            <parrotmessages:ParrotMessageList Name="BlockedList" Access="Protected"></parrotmessages:ParrotMessageList>
        </TabContainer>
    </PanelContainer>
</ui:FancyWindow>
